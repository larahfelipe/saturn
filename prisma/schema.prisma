// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ApplicationError {
  id          String @id @default(uuid())
  name        String
  message     String
  occurredAt  String
  triggeredBy String

  @@map("errors")
}
